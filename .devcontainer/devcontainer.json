{
  "name": "My Stroke Notebook",
  "image": "stroke_analysis:latest",
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "forwardPorts": [8888],
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "settings": {
    "jupyter.notebookFileRoot": "/workspace",
    "python.defaultInterpreterPath": "/usr/local/bin/python"
  }
}
